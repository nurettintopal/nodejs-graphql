    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/polyfill": "^7.8.3",
    "mocha": "^7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nurettintopal/nodejs-graphql-server.git"
