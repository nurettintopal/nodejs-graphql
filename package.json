{
  "name": "nodejs-graphql-server",
  "version": "1.0.0",
  "description": "GraphQL Server Boilerplate for Node.js",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node ./src/index.js",
